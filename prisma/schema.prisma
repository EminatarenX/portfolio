

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model resenia {
  id          Int       @id @default(autoincrement())
  fecha       DateTime @default(now())
  nombre      String
  comentario  String
  calificacion Int
}

model usuario {
  id String @id @default(uuid())
  nombre   String
  password String
  
}

model mensaje {
  id          Int       @id @default(autoincrement())
  fecha       DateTime @default(now())
  nombre      String
  correo  String
  mensaje String
  telefono Int
}